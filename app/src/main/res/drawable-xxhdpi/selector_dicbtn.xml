<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- selector가 눌렸을때 @drawable/dicr 를 보여준다.-->
    <item android:state_pressed="true" android:drawable="@drawable/dicr"/>

    <!-- selector가 눌리지 않았을때 @drawable/dicl 를 보여준다.-->
    <item android:state_pressed="false" android:drawable="@drawable/dicb"/>

</selector>